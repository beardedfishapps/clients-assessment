{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "auditfix": "yarn yarn-audit-fix",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "server": "node server/index.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "checks": "yarn cspell ; yarn lint ; yarn jscpd",
    "cspell": "cspell -u --gitignore .",
    "jscpd": "jscpd .",
    "lint": "yarn lint:js ; yarn lint:prettier ; yarn lint:style",
    "lint:js": "eslint -f compact .",
    "lint:prettier": "prettier --check .",
    "lint:style": "stylelint -f compact \"**/*.css\"",
    "lintfix": "yarn lint:js --fix ; prettier --write --list-different . ; yarn lint:style --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/node": "^18.17.3",
    "cspell": "^6.31.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "jscpd": "^3.5.9",
    "nuxt": "^3.6.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "yarn-audit-fix": "^9.3.12"
  }
}
